{
  "extends": "tslint:recommended",
  "linterOptions": {
    "exclude": ["demo"]
  },
  "rules": {
    "ban-types": false,
    "quotemark": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "prefer-conditional-expression": false,
    "no-console": false,
    "no-debugger": false,
    "object-literal-sort-keys": false
  }
}
